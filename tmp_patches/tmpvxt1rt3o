from neurogym.wrappers.block import TrialHistoryV2 as TrialHistory

class Identity:
    def __init__(self, env, id_=''):
        self.env = env
        self.id = id_

    def reset(self, **kwargs):
        return self.env.reset(**kwargs)

    def step(self, action):
        return self.env.step(action)

class CatchTrials:
    def __init__(self, env, catch_prob=0.1, alt_rew=0):
        self.env = env
        self.catch_prob = catch_prob
        self.alt_rew = alt_rew

    def reset(self, **kwargs):
        return self.env.reset(**kwargs)

    def step(self, action):
        return self.env.step(action)
